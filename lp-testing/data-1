create table lpquestions
(qid int(11) not null auto_increment,
category varchar(100),
question varchar(300),
op_one varchar(50),
op_two varchar(50),
op_three varchar(50),
op_four varchar(50),
answer varchar(50),
expl_a varchar(1000),
primary key(qid));

